{
    "name": "sass.css.builder",
    "version": "0.0.1",
    "private": true,
    "author": "Kaspars@Kamernet",
    "scripts": {
      "pre:iconNames": "node ./Scripts/redesign/css-builder/nameExport.js > ./Content/Sass/icons/_names-array.scss",
      "pre:iconVars": "node ./Scripts/redesign/css-builder/index.js > ./Content/Sass/icons/_icon-variables.scss",
      "pre:iconFallback": "node ./Scripts/redesign/css-builder/index.js no-svg > ./Content/css_redesign/icons-fallback.css",
      "build:mediaQueries": "node ./Scripts/redesign/css-builder/mq-cshtml.js -helpers --prefix /Views/Redesign/Generated/_Media_Queries.cshtml",
      "build:main": "node-sass ./Content/Sass/main.scss ./Content/css_redesign/main.css",
      "build:icons": "node-sass ./Content/Sass/base/_icons.scss ./Content/css_redesign/icons.css",
      "post:main": "node ./node_modules/postcss-cli ./Content/css_redesign/main.css -o ./Content/css_redesign/main.min.css",
      "post:icons": "node ./node_modules/postcss-cli ./Content/css_redesign/icons.css -o ./Content/css_redesign/icons.min.css",
      "post:iconFallback": "node ./node_modules/postcss-cli ./Content/css_redesign/icons-fallback.css -o ./Content/css_redesign/icons-fallback.min.css",
      "critical": "node ./Scripts/redesign/css-builder/critical.js ./Content/css_redesign/full.css ../Qmulus.Kamernet.Web/Views/Redesign/Generated/_Critical_Css.cshtml http://localhost",
      "prebuild": "npm run pre:iconNames & (npm run pre:iconVars && npm run pre:iconFallback)",
      "build": "(npm run build:main && npm run build:icons) && npm run build:mediaQueries",
      "postbuild": "(npm run post:main & npm run post:icons & npm run post:iconFallback) && npm run update",
      "update": "node ./Scripts/redesign/css-builder/updater.js",
      "localUpdate": "SET NODE_ENV=localhost&&SET NODE_CB_EXPORT=../Qmulus.Kamernet.Web&& npm run update",
      "forceUpdate": "SET NODE_ENV=localhost&&SET NODE_CB_EXPORT=../Qmulus.Kamernet.Web&&node ./Scripts/redesign/css-builder/updater.js --force",
      "local": "SET NODE_ENV=localhost&&SET NODE_CB_EXPORT=../Qmulus.Kamernet.Web&& npm run build",
      "prod": "SET NODE_ENV=production&& SET NODE_CB_EXPORT=../Qmulus.Kamernet.Web&& npm run build"
    },
    "devDependencies": {
      "autoprefixer": "^9.7.6",
      "critical": "^2.0.3",
      "cssnano": "^4.1.10",
      "mini-svg-data-uri": "^1.1.3",
      "node-sass": "^4.14.0",
      "node-sass-watcher": "^0.5.1",
      "penthouse": "2.4.0-rc3",
      "postcss": "^7.0.27",
      "postcss-cli": "^7.1.1",
      "postcss-extract-media-query": "^2.0.0",
      "postcss-move-media": "^1.0.0-pre",
      "postcss-sort-media-queries": "^1.7.26",
      "puppeteer": "^5.3.1",
      "request-promise": "^4.2.6",
      "sass-inline-svg": "^1.2.3",
      "watch": "^1.0.2"
    }
  }
  